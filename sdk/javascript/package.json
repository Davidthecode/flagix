{
  "name": "@flagix/js-sdk",
  "version": "1.3.1",
  "description": "Flagix Javascript SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "sdk",
    "javascript",
    "typescript",
    "flagix"
  ],
  "author": "Flagix",
  "license": "ISC",
  "dependencies": {
    "@flagix/evaluation-core": "workspace:^",
    "eventemitter3": "^5.0.1",
    "eventsource": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
