{
  "name": "@flagix/api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "ts-node-runner": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "nodemon-runner": "nodemon --exec \"npm run ts-node-runner --\"",
    "start:api": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "start": "npm run build && concurrently \"npm run start:api\" \"npm run start:worker\"",
    "dev:api": "npm run nodemon-runner -- src/index.ts",
    "dev:worker": "npm run nodemon-runner -- src/worker.ts",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@flagix/data-sync": "workspace:*",
    "@flagix/db": "workspace:*",
    "@react-email/components": "^0.5.7",
    "better-auth": "^1.3.29",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.2.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^22.15.3",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
